pipeline
{
    agent any
    tools
    {
        maven 'Maveen3.6.3'
    }
    
    stages
    {
        stage("data fetching")
        {
            steps
            {
                git 'https://github.com/kshatriyasravi/app-server.git'
            }
        }
        stage("building jar")
        {
            steps
            {
                sh "mvn clean package"
            }
        }
    }
}
